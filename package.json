{
    "name": "frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "vue": "^2.5.22",
        "vue-cookie": "^1.1.4",
        "vue-meta": "^1.5.8",
        "vue-notification": "^1.3.20",
        "vue-resource": "^1.5.1",
        "vue-router": "^3.0.2",
        "vuelidate": "^0.7.6",
        "vuex": "^3.1.0",
        "webpack-svgstore-plugin": "^4.1.0"
    },
    "devDependencies": {
        "style-loader": "^0.23.1",
        "css-loader": "^2.1.0",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "@vue/cli-plugin-babel": "^3.0.5",
        "@vue/cli-plugin-eslint": "^3.0.5",
        "@vue/cli-service": "^3.0.5",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "vue-template-compiler": "^2.5.21"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-mixed-spaces-and-tabs": 0,
            "generator-star-spacing": "off",
            "camelcase": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
